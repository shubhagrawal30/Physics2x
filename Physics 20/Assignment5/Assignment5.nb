Cell[BoxData[
 RowBox[{
  RowBox[{"SerCos", "[", 
   RowBox[{"x_", ",", "n_"}], "]"}], ":=", " ", 
  RowBox[{
   RowBox[{"Normal", "[", 
    RowBox[{"Series", "[", 
     RowBox[{
      RowBox[{"Cos", "[", "y", "]"}], ",", " ", 
      RowBox[{"{", 
       RowBox[{"y", ",", " ", "0", ",", " ", "n"}], "}"}]}], "]"}], "]"}], "/.",
    " ", 
